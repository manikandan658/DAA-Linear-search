#include <stdio.h>
int main() {
   int a[10],i,key,n=6,flag=0;
   printf("enter the array elements:");
   for(i=0;i<n;i++)
   scanf("%d",&a[i]);
   printf("enter the key value:");
   scanf("%d",&key);
   for(i=0;i<n;i++){
       if(key==a[i])
  {
   printf("element found at location %d",i);
   flag=1;
   break;
   }
     }
    if(flag==0)
    printf("element not found");
}
